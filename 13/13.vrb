\frametitle{Co-evolution Methods\small {-Dynamic Grouping}}
\begin{algorithm}
    \caption{A}
    \label{alg:A}
    \begin{algorithmic}
    \STATE {set $r(t)=x(t)$}
    \REPEAT
    \STATE set $h(t)=r(t)$
    \REPEAT
    \STATE set $h(t)=r(t)$
    \UNTIL{B}
    \UNTIL{B}
    \end{algorithmic}
    \end{algorithm}
